@import url("http://fonts.googleapis.com/css?family=Open\+Sans:400,600,700,300")

// Custom Colors
bblue = #33a9e0
bblack = #333
bgreen = #8ac730
bgreen-dark = #78ba17
borange = #f75320
byellow = #e0c52d
grey_9 = #999
grey_6 = #666
grey_4 = #444
grey_3 = #333
grey_c = #ccc
grey_d = #ddd
grey_e = #eee
grey_fa = #fafafa

// Font Styles
open-sans = 'Open Sans', sans-serif

lucida = "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif
lucida-bold = font-weight bold

circle-labels(color)
  content ' '
  background color
  display block
  height 10px
  width 10px
  border-radius 10px
  box-shadow inset 0 0 0 1px rgba(0,0,0,0.1)
  position absolute